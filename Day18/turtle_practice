import turtle as t
from random import randint, choice

toto = t.Turtle()
toto.speed(0)
t.colormode(255)

def color():
    r = randint(0, 255)
    b = randint(0, 255)
    g = randint(0, 255)
    return (r,g,b)

for _ in range(360//5):
    toto.color(color())
    toto.circle(100)
    toto.left(5)
    

screen = t.Screen()
screen.exitonclick()

